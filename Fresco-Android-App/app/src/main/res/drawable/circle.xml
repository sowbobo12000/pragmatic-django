            android:shape="oval">
            <stroke android:width="1dp"
                android:color="#0A000000" />
            <size
                android:width="96dp"
                android:height="96dp"/>
        </shape>
    </item>
    <item
        android:top="6dp"
        android:right="3dp"
        android:bottom="3dp"
        android:left="3dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp"
                android:color="#0F000000" />
