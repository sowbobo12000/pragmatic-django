            android:shape="oval">
            <stroke android:width="1dp"
                android:color="#1A000000" />
            <size
                android:width="96dp"
                android:height="96dp"/>
        </shape>
    </item>
    <item
        android:top="6dp"
        android:right="6dp"
        android:bottom="6dp"
        android:left="6dp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/fresco_yellow"/>
            <size
                android:width="96dp"
                android:height="96dp"/>
        </shape>
    </item>
